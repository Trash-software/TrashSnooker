<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="trashsoftware.trashSnooker.fxml.AchievementsView"
      prefHeight="640.0" prefWidth="1080.0" spacing="10.0">
    
    <padding>
        <Insets topRightBottomLeft="5.0"/>
    </padding>
    
    <HBox spacing="20.0">
        <Button text="%backIcon" onAction="#backAction"/>
        <Label fx:id="totalCountLabel"/>
    </HBox>
    
    <HBox spacing="10.0" VBox.vgrow="ALWAYS">
        <TableView fx:id="achCatTable"/>
        <TableView fx:id="achTable" HBox.hgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="textCol" prefWidth="500.0"/>
                <TableColumn fx:id="dateCol" prefWidth="200.0"/>
                <TableColumn fx:id="iconCol" prefWidth="100.0"/>
            </columns>
        </TableView>
    </HBox>

</VBox>
